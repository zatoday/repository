{
    "name": "zatoday/repository",
    "description": "package repository for laravel",
    "type": "library",
    "keywords": ["laravel","repository","repositories","laravel5.5","laravel5"],
    "support": {
        "issues": "https://github.com/zatoday/repository/issues",
        "source": "https://github.com/zatoday/repository"
    },
    "homepage": "https://zatoday.com",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "ZA",
            "email": "za@zatoday.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ZAToday\\Repository\\RepositoryServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5",
        "illuminate/database": "^5.5",
        "illuminate/container": "^5.5",
        "illuminate/console": "^5.5"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "ZAToday\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZAToday\\Repository\\Tests\\": "tests/"
        }
    }
}
